{
    "compilerOptions": {
      "module": "commonjs",
      "outDir": "dist",
      "noEmit": false,
      "allowJs": false,
      "baseUrl": ".",
      "target": "es5",
      "skipLibCheck": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "incremental": true
    },
}